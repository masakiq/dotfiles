{
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.vimrc",
  "vim.autoindent": true,
  "vim.hlsearch": true,
  // "vim.foldfix": true,
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklasdgjf",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-n>"
      ],
      "after": [
        "g",
        "b"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "s"
      ],
      "commands": [
        "search.action.openNewEditor",
        "workbench.action.search.toggleQueryDetails",
        "toggleSearchEditorCaseSensitive",
        "toggleSearchEditorRegex",
        "toggleSearchEditorWholeWord"
      ]
    },
    {
      "before": [
        "<space>",
        "f",
        "s"
      ],
      "commands": [
        "search.action.focusQueryEditorWidget"
      ]
    },
    {
      "before": [
        "<space>",
        "'"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<space>",
        ";"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "<space>",
        "<space>"
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "e"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    {
      "before": [
        "<space>",
        "c",
        "p"
      ],
      "commands": [
        "copyRelativeFilePath"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "r"
      ],
      "commands": [
        "workbench.action.replaceInFiles",
        "workbench.action.search.toggleQueryDetails"
      ]
    },
    {
      "before": [
        "<space>",
        "a",
        "q"
      ],
      "commands": [
        "workbench.action.closeOtherEditors"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        "'"
      ],
      "commands": [
        "workbench.action.moveEditorRightInGroup"
      ]
    },
    {
      "before": [
        "<space>",
        "m",
        ";"
      ],
      "commands": [
        "workbench.action.moveEditorLeftInGroup"
      ]
    },
    {
      "before": [
        "<space>",
        "p",
        "a"
      ],
      "commands": [
        "workbench.action.pinEditor"
      ]
    },
    {
      "before": [
        "<space>",
        "p",
        "u"
      ],
      "commands": [
        "workbench.action.unpinEditor"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "p"
      ],
      "commands": [
        "workbench.action.openRecent"
      ]
    },
    {
      "before": [
        "<space>",
        "d",
        "n"
      ],
      "commands": [
        "notifications.clearAll"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "b"
      ],
      "commands": [
        "openInGithub.openInGitHubBlame"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "f"
      ],
      "commands": [
        "openInGithub.openInGitHubFile"
      ]
    },
    {
      "before": [
        "<space>",
        "g",
        "h"
      ],
      "commands": [
        "openInGithub.openInGitHubHistory"
      ]
    },
    {
      "before": [
        "f"
      ],
      "after": [
        "<space>",
        "<space>",
        "s"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    // カーソル位置の単語をマルチカーソルで選択
    {
      "before": [
        "<C-n>"
      ],
      "after": [
        "g",
        "b"
      ]
    },
    {
      "before": [
        "m",
        "u"
      ],
      "commands": [
        "extension.changeCase.snake"
      ]
    },
    {
      "before": [
        "m",
        "U"
      ],
      "commands": [
        "extension.changeCase.pascal"
      ]
    },
    {
      "before": [
        "<space>",
        "o",
        "s",
      ],
      "commands": [
        "search.action.openNewEditor",
        "workbench.action.search.toggleQueryDetails",
        "toggleSearchEditorCaseSensitive",
        "toggleSearchEditorRegex",
        "toggleSearchEditorWholeWord",
      ],
    },
    {
      "before": [
        "<space>",
        "o",
        "r",
      ],
      "commands": [
        "workbench.action.replaceInFiles",
        "workbench.action.search.toggleQueryDetails",
      ]
    },
    {
      "before": [
        "=",
      ],
      "commands": [
        "editor.action.formatDocument",
      ]
    },
    {
      "before": [
        "<space>",
        "=",
      ],
      "commands": [
        "editor.action.formatSelection",
      ]
    },
    {
      "before": [
        "m",
        "n",
      ],
      "commands": [
        "editor.action.rename",
      ]
    },
    {
      "before": [
        "m",
        "-",
      ],
      "commands": [
        "extension.changeCase.no",
      ]
    },
  ],
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#8FBCBB",
    "#434C5E"
  ],
  "vim.statusBarColors.insert": "#BF616A",
  "vim.statusBarColors.visual": "#B48EAD",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.easymotionMarkerBackgroundColor": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "workbench.colorCustomizations": {
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E"
  },
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "vim.camelCaseMotion.enable": true,
  "solargraph.autoformat": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.useBundler": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.cursorBlinking": "solid",
  "editor.multiCursorModifier": "alt",
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": false,
  "editor.columnSelection": false,
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "[ruby]": {
    "editor.formatOnSave": true
  },
  "editor.formatOnSaveTimeout": 3000,
  "editor.folding": false,
  "editor.glyphMargin": false,
  "timeline.excludeSources": [
    "git-history"
  ],
  "window.zoomLevel": 1,
  "search.searchEditor.defaultNumberOfContextLines": 2,
  "editor.cursorSurroundingLines": 15,
  "explorer.confirmDelete": false,
  "search.collapseResults": "alwaysCollapse",
  "workbench.editor.highlightModifiedTabs": true,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  // "CodeActionOnParticipant": false,
  // "ExtHostSaveParticipant": false,
  // "editor.scrollbar.verticalScrollbarSize": 15,
  // "ruby.intellisense": "rubyLocate",
  // "ruby.useLanguageServer": true,
  // "solargraph.definitions": true,
  // "solargraph.diagnostics": true
}